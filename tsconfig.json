{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/models/*": [
        "src/models/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ]
    }
  }
}