{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/consts/*": [
        "src/consts/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/images/*": [
        "src/images/*"
      ]
    }
  }
}